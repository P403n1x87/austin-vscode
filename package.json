{
  "name": "austin-vscode",
  "displayName": "Austin VS Code",
  "publisher": "p403n1x87",
  "description": "Austin extension for VS Code",
  "version": "0.6.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/p403n1x87/austin-vscode"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "python",
    "profiling",
    "performance",
    "devtools"
  ],
  "license": "GPL3",
  "icon": "art/logo.png",
  "activationEvents": [
    "onCommand:austin-vscode.profile",
    "onCommand:austin-vscode.load",
    "onStartupFinished",
    "onView:austinTreeView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Austin",
      "properties": {
        "austin.path": {
          "description": "Specifies the absolute path to the Austin binary. Leave blank to discover from PATH",
          "type": "string"
        },
        "austin.interval": {
          "description": "The sampling interval, in microseconds",
          "type": "number",
          "default": 100
        },
        "austin.mode": {
          "description": "Profiling mode",
          "type": "string",
          "enum": [
            "Wall time",
            "CPU time"
          ],
          "enumDescriptions": [
            "Estimate wall-clock time",
            "Estimate CPU time"
          ],
          "default": "Wall time"
        }
      }
    },
    "viewsContainers": {
      "panel": [
        {
          "id": "flame-graph",
          "title": "Flame Graph",
          "icon": "media/austin.svg"
        }
      ],
      "activitybar": [
        {
          "id": "austin-stats",
          "title": "Austin Stats",
          "icon": "media/austin.svg"
        }
      ]
    },
    "views": {
      "flame-graph": [
        {
          "type": "webview",
          "id": "austin-vscode.flameGraph",
          "name": "Flame Graph",
          "icon": "media/austin.svg",
          "visibility": "hidden"
        }
      ],
      "austin-stats": [
        {
          "id": "austin-vscode.callStacks",
          "name": "Call Stacks",
          "contextualTitle": "Call Stacks"
        },
        {
          "id": "austin-vscode.top",
          "name": "Top",
          "contextualTitle": "Top"
        }
      ]
    },
    "commands": [
      {
        "command": "austin-vscode.profile",
        "title": "Profile with Austin"
      },
      {
        "command": "austin-vscode.load",
        "title": "Load Austin Samples ..."
      }
    ],
    "keybindings": [
      {
        "command": "austin-vscode.load",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a"
      },
      {
        "command": "austin-vscode.profile",
        "key": "shift+f5",
        "mac": "shift+f5",
        "when": "editorLangId == python"
      }
    ],
    "taskDefinitions": [
      {
        "type": "austin",
        "required": [
          "file"
        ],
        "properties": {
          "file": {
            "type": "string",
            "description": "The python file path to profile."
          },
          "args": {
            "type": "array",
            "description": "Optional list of arguments to the task"
          },
          "austinArgs": {
            "type": "array",
            "description": "Optional list of austin command-line options"
          },
          "profileName": {
            "type": "string",
            "description": "Name of the generated profile"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "extensionDependencies": [
    "ms-python.python"
  ],
  "dependencies": {
    "d3": "^6.7.0",
    "d3-flame-graph": "^4.0.6"
  }
}