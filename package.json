{
  "name": "austin-vscode",
  "displayName": "Austin VS Code",
  "publisher": "p403n1x87",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:austin-vscode.profile",
    "onCommand:austin-vscode.load"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Austin",
      "properties": {
        "austin.path": {
          "description": "Specifies the absolute path to the Austin binary. Leave blank to discover from PATH.",
          "type": "string"
        }
      }
    },
    "viewsContainers": {
      "panel": [
        {
          "id": "flame-graph",
          "title": "Flame Graph",
          "icon": "resources/package-explorer.svg"
        }
      ]
    },
    "views": {
      "flame-graph": [
        {
          "type": "webview",
          "id": "austin-vscode.flameGraph",
          "name": "Flame Graph"
        }
      ]
    },
    "commands": [
      {
        "command": "austin-vscode.profile",
        "title": "Profile with Austin"
      },
      {
        "command": "austin-vscode.load",
        "title": "Load Austin Samples"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "extensionDependencies": [
    "ms-python.python"
  ],
  "dependencies": {
    "d3": "^6.7.0",
    "d3-flame-graph": "^4.0.6"
  }
}